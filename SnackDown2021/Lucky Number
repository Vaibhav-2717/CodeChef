#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,A,B,C;
	std::cin >> t;
	while(t--){
	    std::cin >> A >>B>>C;
	    int flag=0;
	    if(A==7||B==7||C==7){
	        flag=1;
	    }
	    if(flag==1){
	        std::cout << "YES" << std::endl;
	    }
	    else{
	        std::cout << "NO" << std::endl;
	    }
	}
	return 0;
}
